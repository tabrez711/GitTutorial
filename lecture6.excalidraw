    {
      "id": "LcdBa7mFhMUeN8j_qilZC",
      "type": "text",
      "x": 354.66668701171875,
      "y": 510.16668701171875,
      "width": 917,
      "height": 659,
      "angle": 0,
      "strokeColor": "#000000",
